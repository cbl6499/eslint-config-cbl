{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "target": "esnext",
      "lib": ["dom", "esnext"],
      "moduleResolution": "node",
      "noEmit": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "strict": true,
      "experimentalDecorators": true,
      "useDefineForClassFields": true,
      "noFallthroughCasesInSwitch": true,
    },
  }